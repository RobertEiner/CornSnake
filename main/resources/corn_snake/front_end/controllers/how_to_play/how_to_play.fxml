        <Label fx:id="howToPlayLabel" prefHeight="400.0" prefWidth="600.0" />
        <ImageView fx:id="homeButton" fitHeight="50.0" fitWidth="50.0" layoutX="536.0" layoutY="14.0" onMouseClicked="#onHomeClick" onMouseEntered="#onHover" onMouseExited="#onUnHover" />
